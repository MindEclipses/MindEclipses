const int FRAME_COUNT3 = sizeof(frames3) / sizeof(frames3[0]);


void animasi1() {
  for (int frame = 0; frame < FRAME_COUNT1; frame++) {
    display.clearDisplay();
    display.drawBitmap(40, 8, frames1[frame], 48, 48, 1);
    display.display();
    delay(FRAME_DELAY);
  }
}


void tampilkanTeksTengah(const char* teks) {
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(SSD1306_WHITE);

  int16_t x1, y1;
  uint16_t w, h;
  display.getTextBounds(teks, 0, 0, &x1, &y1, &w, &h);

  int x = (display.width() - w) / 2;
  int y = (display.height() - h) / 2;

  display.setCursor(x, y);
  display.print(teks);
  display.display();
}

void tampilkanSemuaTeksSekali() {
  for (int i = 0; i < totalKalimat; i++) {
    tampilkanTeksTengah(kalimat[i]);

    int panjang = strlen(kalimat[i]);
    int waktuTunggu = panjang * waktuPerKarakter;
    if (waktuTunggu < waktuMinimum) waktuTunggu = waktuMinimum;
    if (waktuTunggu > waktuMaksimum) waktuTunggu = waktuMaksimum;

    delay(waktuTunggu);

    for (int j = 0; j < sizeof(akhirParagraf) / sizeof(akhirParagraf[0]); j++) {
      if (i == akhirParagraf[j]) {
        delay(jedaParagraf); // jeda tambahan antar paragraf
        break;
      }
    }
  }
}



void animasi2() {
  for (int frame = 0; frame < FRAME_COUNT2; frame++) {
    display.clearDisplay();
    display.drawBitmap(48, 16, frames2[frame], FRAME_WIDTH, FRAME_HEIGHT, 1);
    display.display();
    delay(FRAME_DELAY);
  }
}

void animasi3() {
  for (int frame = 0; frame < FRAME_COUNT3; frame++) {
    display.clearDisplay();
    display.drawBitmap(46, 16, frames3[frame], FRAME_WIDTH, FRAME_HEIGHT, 1); // tengahin
    display.display();
    delay(FRAME_DELAY);
  }
}

bool sudahTampilkanTeks = false;

void setup() {
  Serial.begin(9600);

  // Inisialisasi OLED
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay();
  display.setTextWrap(false);

  // 1. Animasi pembuka
  animasi1();

  // 2. Menampilkan semua teks
  tampilkanSemuaTeksSekali();  
  sudahTampilkanTeks = true;

  // Jeda agar teks bisa terbaca
  delay(500);

  // 3. Animasi kedua
  animasi2();

  // Jeda antar animasi
  delay(500);

  // Animasi3 dipindahkan ke loop()
}

void loop() {
  animasi3(); // Berulang terus
}

